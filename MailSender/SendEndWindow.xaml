<Window x:Class="MailSenderNameSpace.SendEndWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MailSenderNameSpace"
        mc:Ignorable="d"
        Title="SendEndWindow" Height="200" Width="350">
    <Grid>
        <Label x:Name="labelMessage" Content="Работа завершена" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0"/>
        <Button Content="Закрыть" HorizontalAlignment="Center" Margin="0,80,0,0" VerticalAlignment="Center" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
